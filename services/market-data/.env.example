# Market Data Service Environment Configuration

# Server Configuration
NODE_ENV=development
PORT=3008
GRPC_PORT=50059
SERVICE_NAME=market-data

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_NAME=market_data

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Security
JWT_SECRET=your-secret-key
ENCRYPTION_KEY=your-encryption-key

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001

# External API Keys
ALPHA_VANTAGE_API_KEY=your-alpha-vantage-api-key
IEX_API_TOKEN=your-iex-api-token

# Data Fetching Configuration
ENABLE_SCHEDULED_FETCH=true
DATA_FETCH_INTERVAL=30

# Cache Configuration
CACHE_TTL=300
CACHE_MAX_ITEMS=100

# Rate Limiting
THROTTLE_TTL=60000
THROTTLE_LIMIT=100

# WebSocket Configuration
WS_MAX_CONNECTIONS=1000
WS_MAX_SYMBOLS_PER_CLIENT=50

# Service Discovery
CONSUL_HOST=localhost
CONSUL_PORT=8500

# Monitoring
ENABLE_METRICS=false
ENABLE_TRACING=false
METRICS_PORT=9090

# Message Queue
RABBITMQ_URL=amqp://treum_admin:securerabbitpass@localhost:5672/treum_finance

# Logging
LOG_LEVEL=info