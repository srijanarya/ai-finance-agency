{
  "name": "@treum/grpc-contracts",
  "version": "1.0.0",
  "description": "gRPC contracts and proto definitions for TREUM AI Finance microservices",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "scripts": {
    "build": "npm run proto:generate && tsc",
    "proto:generate": "npm run proto:generate-js && npm run proto:generate-ts",
    "proto:generate-js": "../../node_modules/.bin/grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./src/generated --grpc_out=grpc_js:./src/generated --plugin=protoc-gen-grpc=../../node_modules/.bin/grpc_tools_node_protoc_plugin -I ./proto ./proto/*.proto",
    "proto:generate-ts": "../../node_modules/.bin/grpc_tools_node_protoc --plugin=protoc-gen-ts=../../node_modules/.bin/protoc-gen-ts --ts_out=grpc_js:./src/generated -I ./proto ./proto/*.proto",
    "clean": "rimraf dist src/generated",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.0",
    "google-protobuf": "^3.21.0"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.0",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.0",
    "grpc-tools": "^1.12.4",
    "grpc_tools_node_protoc_ts": "^5.3.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.1.3"
  }
}