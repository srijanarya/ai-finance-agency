{
  "name": "treum-algotech",
  "version": "1.0.0",
  "description": "TREUM ALGOTECH - AI-Powered Fintech Platform for Trading Education and Signals",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "db:migrate": "turbo run db:migrate",
    "db:seed": "turbo run db:seed",
    "db:reset": "turbo run db:reset"
  },
  "workspaces": [
    "apps/*",
    "services/*",
    "packages/*"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/node": "^22.11.0",
    "eslint": "^8.57.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "turbo": "^2.1.3",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": "22.11.0",
    "npm": ">=10.0.0"
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": ["eslint --fix", "prettier --write"],
    "*.{json,css,md}": ["prettier --write"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/treum-algotech/platform.git"
  },
  "keywords": [
    "fintech",
    "trading",
    "education",
    "signals",
    "crypto",
    "AI",
    "platform"
  ],
  "author": "TREUM ALGOTECH",
  "license": "PROPRIETARY"
}